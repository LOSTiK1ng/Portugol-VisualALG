 A <- 5
 B <- 8
 C <- 10
   escreval (A>B)
   escreval (B<C)
   escreval (A>C)
   escreval (A+B>C)
   escreval (A+C=A+A+A)
   escreval (C= A+B)
   escreval (C>=B^A)
   escreval (C<=B^A)
   escreval (B<=C%2)
   escreval (1=C%2)

