Var
// Seção de Declarações das variáveis 
nota:inteiro

notaR:real

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc... 
 notaR <- 10
 
 escreval("Escreva o valor de sua nota:")
 
 leia (nota)
 
 se (nota >= 7) entao
 
    escreval ("Parabéns! Continue assim!")

 se (nota = notaR) entao
 
    escreval ("Parabéns! Voce tirou a maior nota!")
    
 se (nota < 7) entao
 
      escreval ("Voce precisa estudar mais!")
      
 senao
 
      escreval ("Coloque uma nota valida ata 10")
      
      fimse





